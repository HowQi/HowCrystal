{
	//表示注册者是一个"好友"
	type="friend";
	//好友的QQ号
	id="00000000";
	//注册直播间的房间号
	rooms+="00000000";
	//注册直播间的主播自定义称呼(项目中称作Crystal ID,要求唯一)
	//Crystal ID 在直播观测目标中定义。
	//必须以"@"符号开头
	rooms+="@someone";
	//就如上面这样,您可以为一个好友注册多个主播通知
}
{
	//表示注册者是一个"群聊"
	type="group";
	id="00000000";
	//注册房间的房间号
	rooms+="00000000";
	//注册房间的主播的Crystal ID
	rooms+="@someone";
	//就如上面这样,您可以为一个群聊注册多个主播通知
}